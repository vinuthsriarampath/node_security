PORT=server running port
MONGO_URL=mongoDB URL
FRONTEND_URL=frontend url
JWT_SECRET= secret for the JWT ( use node -e "console.log(require('crypto').randomBytes(64).toString('hex'))")
JWT_EXPIRES_IN=jwt expires time (e.g., 1m,5m,15m,30m,1h,2h,12h,1d -> short lived token)
REFRESH_TOKEN_EXPIRES_IN=refresh token expires time (e.g., 7d,30d,90d,1y -> long lived token)
NODE_ENV=development or production